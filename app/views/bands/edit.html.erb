<div id="wrap-content">
  <h4><%= link_to "Back to band page", bands_path %></h4>
  <h1>edit bands</h1>
  <div class="rapp-form">
    <%= simple_form_for [@band] do |f| %>
      <%= f.input :name %>
      <%= f.input :content %>
      <%= f.input :pictures %>
      <%= f.input :soundcloud %>
      <%= f.input :location %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
